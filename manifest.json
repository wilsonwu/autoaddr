{
  "manifest_version": 3,
  "name": "Auto Address",
  "version": "1.0",
  "description": "Extension with Azure OpenAI settings",
  "permissions": ["storage", "sidePanel"],
  "action": {
    "default_title": "Open Auto Address"
  },
  "side_panel": {
    "default_path": "hello.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}
